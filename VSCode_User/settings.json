// Place your settings in this file to overwrite the default settings
{
    "window.zoomLevel": 0,
    "files.exclude": {
      ".eggs/": true,
      ".ipynb_checkpoints/": true,
      ".mypy_cache/": true,
      ".pytest_cache/": true,
      "**/__pycache__": true,
      "**/.coverage.*": true,
      "**/.vscode": true,
      "**/*.aux": true,
      "**/*.bcf": true,
      "**/*.blg": true,
      "**/*.brf": true,
      "**/*.egg-info": true,
      "**/*.fdb_latexmk": true,
      "**/*.fls": true,
      "**/*.pyc": true,
      "**/*.run.xml": true,
      "**/*.synctex.gz": true,
      "**/*.xdv": true,
      "build/": true,
      "dask-worker-space/": true,
      "pelican.pid": true,
      "srv.pid": true,
      "venv*/": true
    },
    "editor.rulers": [80],
    "python.pythonPath": "${workspaceRoot}/venv/bin/python",
    "python.autoComplete.extraPaths": [
        "${workspaceRoot}/venv/lib/python2.7",
        "${workspaceRoot}/venv/lib/python2.7/site-packages"
    ],
    "python.linting.pylintEnabled": true,
    "python.linting.flake8Enabled": false,
    "files.insertFinalNewline": true,
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": false,
    "editor.wordWrap": "on",
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": false,
    "workbench.colorTheme": "Visual Studio Light",
    "typescript.updateImportsOnFileMove.enabled": "never",
    "latex-workshop.message.update.show": false,
    "latex-workshop.latex.recipes": [
      {
        "name": "latexmk",
        "tools": [
          "latexmk"
        ]
      },
      {
        "name": "pdflatex -> biber -> pdflatex*2",
        "tools": [
          "pdflatex",
          "biber",
          "pdflatex",
          "pdflatex"
        ]
      },
    ],
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.latex.tools": [
      {
        "name": "latexmk",
        "command": "latexmk",
        "args": [
          "-f",
          "-synctex=1",
          "-interaction=nonstopmode",
          "-file-line-error",
          "-pdf",
          "%DOC%"
        ]
      },
      {
        "name": "pdflatex",
        "command": "pdflatex",
        "args": [
          "-synctex=1",
          "-interaction=nonstopmode",
          "-file-line-error",
          "%DOC%"
        ]
      },
      {
        "name": "bibtex",
        "command": "bibtex",
        "args": [
          "%DOCFILE%"
        ]
      },
      {
        "name": "biber",
        "command": "biber",
        "args": [
          "%DOCFILE%"
        ]
      }
    ],
    "revealjs.theme": "white",
    "revealjs.slideNumber": true,
    "spellright.configurationScope": "user",
    "spellright.language": [
      "en",
      "de"
    ],
    "pasteImage.path": "${currentFileDir}/images",
    "pasteImage.showFilePathConfirmInputBox": true,
    "terminal.integrated.rendererType": "dom",
    "editor.renderControlCharacters": false,
    "breadcrumbs.enabled": true,
    "editor.renderWhitespace": "none",
    "spellright.documentTypes": [
      "markdown",
      "latex",
      "plaintext"
    ],
    "C_Cpp.updateChannel": "Insiders",
    "files.trimTrailingWhitespace": true,
    "telemetry.enableTelemetry": false,
    "C_Cpp.default.includePath": [
      "/Users/svenkreiss/tech/libtorch/include"
    ],
    "explorer.confirmDragAndDrop": false,
    "latex-workshop.latex.build.clearLog.everyRecipeStep.enabled": false,
    "[latex]": {
      "editor.formatOnPaste": false,
      "editor.suggestSelection": "recentlyUsedByPrefix"
    },
